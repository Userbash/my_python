#SERVER
ip=192.168.62.100
dir=Users1

#Date Formation
data_time_mes=$(date -dm |awk '{print$2}')

#Search proces pid
id=$(pidof cp)

#Squeaking a connected server
mount_f=$(cat /proc/mounts | grep $ip )

