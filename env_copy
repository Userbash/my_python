	 . env_per

	   #UP TEST
       list=$(for i in $(ls -d /root/tmp/*); do echo $i |  sed -e 's|/root/tmp/||g'; done)
         echo "$list" | while read line
            do 
              cat /tmp/dates.log  | while read name
                 do
                  if echo "$name"| grep "$line"
                     then
                       cp -prvf $HOME/tmp/$name $PATCH > $PATCH/backup.log
                  else
                      echo ""
                  fi     
                   done
            done

	   sleep 5 &&\
	   umount -l $HOME/tmp 
	   rm -rf $HOME/tmp